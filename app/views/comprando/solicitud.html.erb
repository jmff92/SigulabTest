		      <li> <%=link_to 'Compras', compras_url %></li>
		      <li> <%=link_to 'Especificaciones Técnicas', specifications_url %></li>
		      <li class="active">  Solicitud  </li>
		   </ol>
	</div>

	<div class="grid_6 push_4">

		<ul id="tittle">
			<%= link_to especificacionesTecnicas_url do %>
		       <li style="text-align:left"><h4><i class="fa fa-clipboard fa-1"> Especificaciones Técnicas </i></h4></li>
		   <% end %>
			<%= link_to invitations_url do %>
		       <li style="text-align:left"><h4><i class="fa fa-envelope-o">&nbsp Carta de invitación </i></h4></li>
		   <% end %>
			<%= link_to quotes_url do %>
